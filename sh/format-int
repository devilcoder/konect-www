#! /bin/sh

sed -E -e '

s/^([^.]*[0-9])([0-9]{27}(\.|$))/\1,\2/
s/^([^.]*[0-9])([0-9]{24}(\.|$))/\1,\2/
s/^([^.]*[0-9])([0-9]{21}(\.|$))/\1,\2/
s/^([^.]*[0-9])([0-9]{18}(\.|$))/\1,\2/
s/^([^.]*[0-9])([0-9]{15}(\.|$))/\1,\2/
s/^([^.]*[0-9])([0-9]{12}(\.|$))/\1,\2/
s/^([^.]*[0-9])([0-9]{9}(\.|$))/\1,\2/
s/^([^.]*[0-9])([0-9]{6}(\.|$))/\1,\2/
s/^([^.]*[0-9])([0-9]{3}(\.|$))/\1,\2/

'

